<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="/stylesheets/Expense/profile.css">
</head>

<body>
    <%- include('./nav.ejs') %>
        <!--  -->
        <div class="add-item">
            <!-- /create-expense -->
            <form action="/create-expense" method="post">
                <select class="form-control w-25" name="category" id="select">
                    <option value="">Select Category</option>
                    <% category.forEach((element)=> { %>
                        <option value="<%= element %>">
                            <%= element %>
                        </option>
                        <% }) %>
                </select>
                <br><br>
                <input id="description" class="form-control" type="text" placeholder="description" name="description">
                <br><br>
                <input class="form-control" type="number" placeholder="amount" name="amount">
                <br><br>
                <button class="btn btn-submit">Submit</button>
            </form>
        </div>

        <!--  -->
        <div class="profile">

        </div>




        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
            crossorigin="anonymous"></script>
</body>

</html>